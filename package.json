{
  "name": "@openchatai/widget",
  "private": false,
  "version": "1.3.9",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rm -rf ./dist ./dist-embed",
    "prepublishOnly": "pnpm clean && pnpm test && pnpm batman",
    "build": "vite build",
    "dev": "vite",
    "build:embed": "vite build -c vite.embed.config.ts",
    "batman": "pnpm build && pnpm build:embed",
    "test": "vitest run",
    "test:watch": "vitest --watch"
  },
  "peerDependencies": {
    "lucide-react": "^0.412.0",
    "react": "^18.3.1",
    "styled-components": "^6.1.13"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@changesets/cli": "^2.27.9",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.2",
    "@testing-library/react": "^16.0.1",
    "@types/node": "^20.14.8",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@types/tinycolor2": "^1.4.6",
    "@vitejs/plugin-react": "^4.3.2",
    "axios": "^1.7.7",
    "class-variance-authority": "^0.7.0",
    "jsdom": "^25.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^9.0.1",
    "remark-gfm": "^4.0.0",
    "socket.io-client": "^4.8.0",
    "styled-components": "^6.1.13",
    "styled-normalize": "^8.1.1",
    "swr": "^2.2.5",
    "tailwind-merge": "^2.5.4",
    "typescript": "^5.6.3",
    "vite": "^5.4.9",
    "vite-plugin-dts": "4.2.4",
    "vite-plugin-externalize-deps": "^0.8.0",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.1.3"
  },
  "files": [
    "dist",
    "dist-embed"
  ],
  "bugs": {
    "url": "https://github.com/openchatai/OpenCopilot/issues"
  },
  "author": {
    "name": "openchatai",
    "url": "https://github.com/openchatai"
  },
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/index.d.ts"
    },
    "./basic": {
      "import": "./dist/basic.es.js",
      "require": "./dist/basic.cjs.js",
      "types": "./dist/basic.d.ts"
    },
    "./package.json": "./package.json",
    "./dist/*.css": "./dist/*.css"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.1",
    "@radix-ui/react-popover": "^1.1.2",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.1",
    "@radix-ui/react-toast": "^1.2.2",
    "@radix-ui/react-tooltip": "^1.1.3",
    "@tiptap/core": "^2.8.0",
    "@tiptap/extension-placeholder": "^2.8.0",
    "@tiptap/pm": "^2.8.0",
    "@tiptap/react": "^2.8.0",
    "@tiptap/starter-kit": "^2.8.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^11.11.9",
    "immer": "^10.1.1",
    "lucide-react": "^0.453.0",
    "mitt": "^3.0.1",
    "tinycolor2": "^1.6.0",
    "wouter": "^3.3.5",
    "zod": "^3.23.8"
  }
}